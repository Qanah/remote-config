{
    "name": "jawabapp/remote-config",
    "description": "A standalone Laravel package for A/B testing, remote configuration, and feature experimentation",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ab-testing",
        "remote-config",
        "experiments",
        "feature-flags",
        "configuration"
    ],
    "authors": [
        {
            "name": "Jawab Team",
            "email": "dev@jawab.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "jfcherng/php-diff": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Jawabapp\\RemoteConfig\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Jawabapp\\RemoteConfig\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jawabapp\\RemoteConfig\\RemoteConfigServiceProvider"
            ],
            "aliases": {
                "RemoteConfig": "Jawabapp\\RemoteConfig\\Facades\\RemoteConfig"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
